public class Solution {
    public int solve(int[] A, int[] B, int C) {
        int count1 = 0;
        int count2=0;
        for(int i = 0 ; i<A.length;i++){
            if(A[i]>C){
                count1++;
            }}
             for(int i = 0 ; i<B.length;i++){
            if(B[i]<C){
                count2++;
            }
        }if(count1 > count2){
         return count1;}
         else{
             return count2;
         }
    }
   
}
